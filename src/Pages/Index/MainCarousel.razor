<GenericCarousel CarouselId="carouselVarejo">
    <IndicatorsTemplate>
        <li data-target="#carouselVarejo" data-slide-to="0" class="active"></li>
        <li data-target="#carouselVarejo" data-slide-to="1"></li>
        <li data-target="#carouselVarejo" data-slide-to="2"></li>
    </IndicatorsTemplate>
    <ImagesTemplate>
        <div class="carousel-item active">
            <img class="imagem-carrossel d-block w-100" src="images/mercado.png" alt="Primeiro slide">
        </div>
        <div class="carousel-item">
            <img class="imagem-carrossel d-block w-100" src="images/mercado_2.png" alt="Segundo slide">
        </div>
        <div class="carousel-item">
            <img class="imagem-carrossel d-block w-100" src="images/mercado_3.png" alt="Terceiro slide">
        </div>
    </ImagesTemplate>
</GenericCarousel>